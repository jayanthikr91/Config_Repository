node{

checkout scm;
def prop=readProperties  file: 'Properties/pipeline.properties'

  
stage('clear workspace')
{
  deleteDir()
}  
stage('util clone')
{
sh prop.UTIL_REPO_URL
echo "cloned successfully"
utilrepo=load prop.JAVA_GROOVY_FILE_LOC
commonutility=load prop.COMMON_GROOVY_FILE_LOC
  echo "${commonutility}"
utilrepo.execute(prop);  
}
}
