node{

checkout scm;
def prop=readProperties  file: 'Properties/pipeline.properties'
def utilrepo=load prop.JAVA_GROOVY_FILE_LOC
def commonrepo=load prop.COMMON_GROOVY_FILE_LOC
  
stage('clear workspace')
{
  commonrepo.clearWorkspace()
}  
stage('util clone')
{
sh prop.GIT_CLONE_CMD+prop.UTIL_REPO_URL
echo "cloned successfully"
utilrepo.execute(prop);  
}
}
